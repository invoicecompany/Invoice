<% if customer_signed_in? %>
    <h1>Ya has ingresa session como Microempresa </h1>
<% else %>
<div class="formulario">
<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label :email, :class=>"etiqueta" %><br />
    <%= f.email_field :email, :class=>"email", autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password, :class=>"etiqueta" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password,:class=>"pass", autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation, :class=>"etiqueta" %><br />
    <%= f.password_field :password_confirmation,:class=>"pass", autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up", :class=>"enviar" %>
  </div>
<% end %>

<%= render "dealers/shared/links" %>
<% end %>
</div>